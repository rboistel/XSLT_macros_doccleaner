    <w:footnote w:id="12">
        <w:p w:rsidR="00537922" w:rsidRPr="005643D2" w:rsidRDefault="00537922" w:rsidP="00101207">
            <w:pPr>
                <w:pStyle w:val="Notedebasdepage"/>
            </w:pPr>
            <w:r w:rsidRPr="005643D2">
                <w:rPr>
                    <w:vertAlign w:val="superscript"/>
                </w:rPr>
                <w:t>12</w:t>
            </w:r>
            <w:r w:rsidRPr="005643D2">
                <w:t xml:space="preserve"> Dans les dix dernières saisons, l’ASSE a plus évolué en seconde division qu’en première.</w:t>
            </w:r>
        </w:p>
    </w:footnote>
    <w:footnote w:id="13">
        <w:p w:rsidR="00537922" w:rsidRPr="005643D2" w:rsidRDefault="00537922" w:rsidP="00101207">
            <w:pPr>
                <w:pStyle w:val="Notedebasdepage"/>
            </w:pPr>
            <w:r w:rsidRPr="005643D2">
                <w:rPr>
                    <w:vertAlign w:val="superscript"/>
                </w:rPr>
                <w:t>13</w:t>
            </w:r>
            <w:r w:rsidRPr="005643D2">
                <w:t xml:space="preserve"> Rappelons que les forces vives de supporters de l’ASSE sont plutôt jeunes et n’ont pas connu l’Epopée.</w:t>
            </w:r>
        </w:p>
    </w:footnote>
    <w:footnote w:id="14">
        <w:p w:rsidR="00537922" w:rsidRPr="005643D2" w:rsidRDefault="00537922" w:rsidP="00101207">
            <w:pPr>
                <w:pStyle w:val="Notedebasdepage"/>
            </w:pPr>
            <w:r w:rsidRPr="005643D2">
                <w:rPr>
                    <w:vertAlign w:val="superscript"/>
                </w:rPr>
                <w:t>14</w:t>
            </w:r>
            <w:r w:rsidRPr="005643D2">
                <w:t xml:space="preserve"> Et même au delà avec de grands travaux de voirie en entrée de ville, en venant de Saint-Chamond sur la RN88.</w:t>
            </w:r>
        </w:p>
    </w:footnote>
    <w:footnote w:id="15">
        <w:p w:rsidR="00537922" w:rsidRPr="005643D2" w:rsidRDefault="00537922" w:rsidP="00101207">
            <w:pPr>
                <w:pStyle w:val="Notedebasdepage"/>
            </w:pPr>
            <w:r w:rsidRPr="005643D2">
                <w:rPr>
                    <w:vertAlign w:val="superscript"/>
                </w:rPr>
                <w:t>15</w:t>
            </w:r>
            <w:r w:rsidRPr="005643D2">
                <w:t xml:space="preserve"> Grâce à la venue de nombreux Ecossais au moment du Mondial et grâce à l’instauration d’une ligne aérienne Saint-Etienne / Londres par la compagnie Ryanair.</w:t>
            </w:r>
        </w:p>
    </w:footnote>
    <w:footnote w:id="16">
        <w:p w:rsidR="00537922" w:rsidRPr="005643D2" w:rsidRDefault="00537922" w:rsidP="00101207">
            <w:pPr>
                <w:pStyle w:val="Notedebasdepage"/>
            </w:pPr>
            <w:r w:rsidRPr="005643D2">
                <w:rPr>
                    <w:vertAlign w:val="superscript"/>
                </w:rPr>
                <w:t>16</w:t>
            </w:r>
            <w:r w:rsidRPr="005643D2">
                <w:t xml:space="preserve"> La Coupe du Monde gardant un caractère exceptionnel comme l’a bien montré le succès en demi-teinte, en terme de communication touristico-sportive, de la Coupe des Confédérations en 2003.</w:t>
            </w:r>
        </w:p>
    </w:footnote>
